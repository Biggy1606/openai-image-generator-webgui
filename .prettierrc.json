{
  "useTabs": true,
  "tabWidth": 4,
  "semi": true,
  "singleQuote": false,
  "printWidth": 80,
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "eslintIntegration": true
}
